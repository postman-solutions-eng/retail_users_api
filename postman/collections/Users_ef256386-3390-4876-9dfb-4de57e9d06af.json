        {
            "name": "Get all user segments",
            "event": [
                {
                    "listen": "test",
                    "script": {
                        "id": "9c6525c9-7b6d-45de-9ed6-ce40350720d6",
                        "exec": [
                            "var jsonData = pm.response.json();",
                            "",
                            "pm.test(\"Status code is 200\", function () {",
                            "    pm.response.to.have.status(200);",
                            "});",
                            "",
                            "pm.test(\"Expect a segments array\", function () {",
                            "    pm.expect(jsonData).to.have.property(\"segments\");",
                            "});",
                            "",
                            "pm.test(\"Expect items to have an ID and quantity\", function () {",
                            "    for (var i = 0; i < jsonData.segments.length; i++) {",
                            "        pm.expect(jsonData.segments[i]).to.include.all.keys(\"name\");",
                            "    }",
                            "});",
                            "",
                            "pm.test(\"Expect item name to be string\", function () {",
                            "    for (var i = 0; i < jsonData.segments.length; i++) {",
                            "        pm.expect(jsonData.segments[i].name).to.be.a('string');",
                            "    }",
                            "});"
                        ],
                        "type": "text/javascript"
                    }
                }
            ],
